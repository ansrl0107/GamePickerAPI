{
    "title": "Get all questions",
    "endpoint": "/admin/questions",
    "method": "get",
    "request": {
        "headers": {
            "x-access-token": "Token from '/auth/login'"
        },
        "query": {
            "limit": "Limit the number of data",
            "offset": "Set start index, required 'limit'"
        }
    },
    "response": [
        {
            "status": 200,
            "data": {
                "questions": [
                    {
                        "id": 1,
                        "title": "title of question",
                        "email": "email to reply",
                        "value": "context of question"
                    }
                ]
            }
        },
        {
            "status": 400,
            "data": {
                "message": "Token is required"
            }
        },
        {
            "status": 400,
            "data": {
                "message": "Token is invalid"
            }
        },
        {
            "status": 401,
            "data": {
                "message": "Admin authentication required"
            }
        },
        {
            "status": 404,
            "data": {
                "message": "Question not found"
            }
        }
    ]
}