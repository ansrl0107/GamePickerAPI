{
    "title": "Delete notice",
    "endpoint": "/admin/notices/:notice_id",
    "method": "delete",
    "request": {
        "headers": {
            "x-access-token": "Token from '/auth/login'"
        }
    },
    "response": [
        {
            "status": 204,
            "data": null
        },
        {
            "status": 400,
            "data": {
                "message": "'x-access-token' is required."
            }
        },
        {
            "status": 400,
            "data": {
                "message": "'x-access-token' is included, but can not be decoded."
            }
        },
        {
            "status": 404,
            "data": {
                "message": "Can not find user matching token. It may be an old token."
            }
        },
        {
            "status": 404,
            "data": {
                "message": "Can not find notice matching 'notice_id'."
            }
        }
    ]
}